{
  "name": "christerolsen-me",
  "version": "1.0.0",
  "description": "Homepage for myself",
  "author": "Christer Olsen <colsen1991@gmail.com> (https://www.christerolsen.me)",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "prestaging": "ncp .robots/robots-staging.txt static/robots.txt && ncp .store store",
    "staging": "cross-env STAGING=true npm run generate",
    "preproduction": "ncp .robots/robots-prod.txt static/robots.txt && rimraf store",
    "production": "cross-env STAGING=false npm run generate",
    "generate": "cross-env STATIC=true nuxt generate"
  },
  "dependencies": {
    "@nuxtjs/auth": "4.5.3",
    "@nuxtjs/axios": "5.3.6",
    "@nuxtjs/google-analytics": "2.0.2",
    "@nuxtjs/pwa": "2.6.0",
    "@nuxtjs/sitemap": "0.2.0",
    "bulma": "0.7.2",
    "nuxt": "2.3.4",
    "vue-lazyload": "1.2.6"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "cross-env": "5.2.0",
    "datocms-client": "0.8.11",
    "eslint": "5.12.0",
    "eslint-config-standard": "12.0.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-html": "5.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-standard": "4.0.0",
    "ncp": "2.0.0",
    "node-sass": "4.11.0",
    "rimraf": "2.6.3",
    "sass-loader": "7.1.0"
  }
